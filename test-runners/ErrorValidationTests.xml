<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Error Validation Test Suite"  parallel="tests" thread-count="2">
    <listeners>
        <listener class-name="com.ecommerce.testComponents.Listeners"/>
    </listeners>

    <test name="Verify login using Invalid credentials Test">
        <classes>
            <class name="com.ecommerce.testCases.LoginWithInvalidCredTest"/>
        </classes>
    </test>

    <test name="Register User using Invalid phone Number Test">
        <classes>
            <class name="com.ecommerce.testCases.RegisterUserWithInvalidNumber"/>
        </classes>
    </test>


</suite>